{
  "name": "server",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "scripts": {
    "generate": "graphql-codegen",
    "prisma:generate": "prisma generate --schema=prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=prisma/schema.prisma",
    "prisma:push": "prisma db push --schema=prisma/schema.prisma",
    "prisma:migrate:reset": "prisma migrate reset --schema=prisma/schema.prisma",
    "prebuild": "npm run prisma:generate && npm run generate || true",
    "build": "tsc --pretty --project tsconfig.json",
    "build:prod": "npm run prebuild && tsc -p tsconfig.build.json",
    "start:prod": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run src/**/__tests__/**/*.test.ts --exclude=**/*.integration.test.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "@graphql-tools/schema": "^10.0.6",
    "@graphql-tools/utils": "^10.3.2",
    "@parcel/watcher": "^2.4.1",
    "@prisma/client": "^6.2.1",
    "bcrypt": "^5.1.1",
    "concurrently": "^9.1.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "graphql": "^16.10.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-scalars": "^1.24.2",
    "graphql-tag": "^2.12.6",
    "graphql-voyager": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^15.5.0",
    "lodash": "^4.17.21",
    "nodemailer": "^7.0.3",
    "redis": "^5.9.0",
    "regex": "^6.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/client-preset": "^4.8.0",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.0",
    "@graphql-codegen/typescript-resolvers": "^4.2.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/graphql-depth-limit": "^1.1.6",
    "@types/graphql-upload": "^17.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.16",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^22.14.0",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.5",
    "@vitest/ui": "^4.0.5",
    "prettier": "^3.5.3",
    "prisma": "^6.2.1",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vitest": "^4.0.5",
    "vitest-mock-extended": "^3.1.0"
  },
  "engines": {
    "node": ">=22.14.0"
  },
  "packageManager": "npm@10.9.2"
}
